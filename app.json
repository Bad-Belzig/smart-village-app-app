{
  "expo": {
    "name": "MeinQuartierX",
    "slug": "meinquartier",
    "owner": "ikusei",
    "privacy": "unlisted",
    "description": "An open source React Native app for smart villages merging regional offers and services in one place - in your pocket.",
    "githubUrl": "https://github.com/ikuseiGmbH/smart-village-app-app",
    "platforms": ["ios", "android"],
    "version": "3.3.3",
    "extra": { "eas": { "projectId": "84611212-b1ac-47b3-8f2d-065898fab1cb" } },
    "updates": { "url": "https://u.expo.dev/84611212-b1ac-47b3-8f2d-065898fab1cb" },
    "runtimeVersion": { "policy": "nativeVersion" },
    "orientation": "portrait",
    "primaryColor": "#3F7193",
    "icon": "./assets/icon.png",
    "scheme": "mq",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "de.test.hb.meinquartier.smartvillage.app",
      "buildNumber": "14",
      "supportsTablet": true,
      "usesIcloudStorage": true,
      "requireFullScreen": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Diese App nutzt die Kamera um QR-Codes zu scannen.",
        "NSPhotoLibraryUsageDescription": "Diese App nutzt die Medienbibliothek f√ºr die Auswahl eines Profilbildes.",
        "NSLocationWhenInUseUsageDescription": "Diese App nutzt die Standortbestimmung um Ihre aktuelle Position auf der Karte darzustellen.",
        "CFBundleLocalizations": ["de"],
        "CFBundleDevelopmentRegion": "de_DE",
        "LSApplicationQueriesSchemes": ["whatsapp"]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "de.test.hb.meinquartier.smartvillage.app",
      "versionCode": 24,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC0P5I0NMEjQS2auZUfK3dzgWwPKGrkllc"
        }
      },
      "softwareKeyboardLayoutMode": "pan",
      "allowBackup": false
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "appleTeamId": "3XB7P8477W"
        }
      ],
      [
        "expo-camera",
        {
          "microphonePermission": false
        }
      ],
      [
        "expo-image-picker",
        {
          "microphonePermission": false
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ],
      "expo-localization",
      "sentry-expo",
      "./config-plugins/withAndroidMailQueriesAndWhatsappPackage"
    ]
  }
}
